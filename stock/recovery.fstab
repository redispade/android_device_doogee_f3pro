#TWRP by redispade
/boot			emmc	/dev/block/platform/mtk-msdc.0/by-name/boot		flags=display="Boot";backup=1
/logo			emmc	/dev/block/platform/mtk-msdc.0/by-name/logo		lags=display="Logo";backup=1;flashimg=1
/nvram			emmc	/dev/block/platform/mtk-msdc.0/by-name/nvram		flags=display="Nvram";backup=1;flashimg=1
/recovery		emmc	/dev/block/platform/mtk-msdc.0/by-name/recovery		flags=display="Recovery";backup=1
/lk			emmc	/dev/block/platform/mtk-msdc.0/by-name/lk
/misc           	emmc    /dev/block/platform/mtk-msdc.0/by-name/para
/system			ext4	/dev/block/platform/mtk-msdc.0/by-name/system
/cache			ext4	/dev/block/platform/mtk-msdc.0/by-name/cache
/data			ext4	/dev/block/platform/mtk-msdc.0/by-name/userdata  	length=-16384
/external_sd		auto	/dev/block/mmcblk1p1	/dev/block/mmcblk1		flags=display="Micro SD";storage;wipeingui;removable;backup=0
/usbstorage		vfat    /dev/block/sda1			/dev/block/sda		flags=display="Usb-Otg";storage;wipeingui;removable;backup=0

